    "eslint-plugin-react-refresh": "^0.4.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.34.0"

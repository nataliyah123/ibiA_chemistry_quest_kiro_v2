{
  "name": "chemquest-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:unit": "vitest --run src/test/unit",
    "test:integration": "vitest --run src/test/integration",
    "test:e2e": "vitest --run src/test/e2e",
    "test:accessibility": "vitest --run src/test/accessibility",
    "test:coverage": "vitest --run --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:accessibility"
  },
  "dependencies": {
